					"command": "viewLaunchs.DeleteEnviroment",
					"when": "view == viewLaunchs && viewItem == iotenviromentitem"
				},
				{
					"command": "viewSBC.CopyToClipboard",
					"when": "view == viewLaunchs && viewItem == iotenviromentitem",
					"group": "navigation"
				},
				{
					"command": "viewSBC.CopyToClipboard",
					"when": "view == viewLaunchs && viewItem == iotenviromentoption",
					"group": "navigation"
				}
			]
		},
