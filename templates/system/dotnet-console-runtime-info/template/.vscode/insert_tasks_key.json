{
    "values": [
        {
			"label": "fastiot-%{launch.id}-build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}%{project.mainfile.path.relative.aslinux}",
				"--configuration",
				"Debug",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary",
				"-r","%{sbc.dotnet.rid}"                
			],
			"problemMatcher": "$msCompile"
		},
        {
			"label": "fastiot-%{launch.id}-create-folder",
			"dependsOn":"fastiot-%{launch.id}-build",
			"command": "%{template.storage.path.aswindows}\\apps\\cwrsync\\ssh.exe",
			"type": "process",
			"args": [
					"-i",
					"%{sbc.ssh.key.path.full.aswindows}",
					"-o",
					"StrictHostKeyChecking no",
					"-p",
					"%{sbc.ssh.port}",
					"%{sbc.user.debug}@%{sbc.host}",
					"mkdir -p %{debug.app.folder}/%{project.name}"
			],
			"problemMatcher": "$msCompile"
		},
        {
			"label": "fastiot-%{launch.id}-copy-app-to-sbc",
			"dependsOn":"fastiot-%{launch.id}-create-folder",
			"command": "%{template.storage.path.aswindows}\\apps\\cwrsync\\rsync.exe",
			"type": "process",
			"args": [
				"--log-file=rsync.log",
				"--progress",
				"-avz" ,
				"-e",
				"%{template.storage.path.aswindows}\\apps\\cwrsync\\ssh.exe -i %{sbc.ssh.key.path.full.aswindows} -o 'StrictHostKeyChecking no' -p %{sbc.ssh.port}",
				"%{project.path.full.ascygdrive}/bin/Debug/%{project.dotnet.targetframework}/%{sbc.dotnet.rid}/",
				"%{sbc.user.debug}@%{sbc.host}:%{debug.app.folder}/%{project.name}"
			],
			 "problemMatcher": "$msCompile"
		},
		{
			"label": "fastiot-%{launch.id}-copy-app-to-sbc-without-logs",
			"dependsOn":"fastiot-%{launch.id}-create-folder",
			"command": "%{template.storage.path.aswindows}\\apps\\cwrsync\\rsync.exe",
			"type": "process",
			"args": [
				"--progress",
				"-avz" ,
				"-e",
				"%{template.storage.path.aswindows}\\apps\\cwrsync\\ssh.exe -i %{sbc.ssh.key.path.full.aswindows} -o 'StrictHostKeyChecking no' -p %{sbc.ssh.port}",
				"%{project.path.full.ascygdrive}/bin/Debug/%{project.dotnet.targetframework}/%{sbc.dotnet.rid}/",
				"%{sbc.user.debug}@%{sbc.host}:%{debug.app.folder}/%{project.name}"
			],
			 "problemMatcher": "$msCompile"
		}
    ]
}